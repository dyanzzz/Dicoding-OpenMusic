{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",
        "max-len": 0,
        "no-underscore-dangle": ["error", { "allow": ["_notes", "_service", "_validator", "_pool"] }]
    }
}
